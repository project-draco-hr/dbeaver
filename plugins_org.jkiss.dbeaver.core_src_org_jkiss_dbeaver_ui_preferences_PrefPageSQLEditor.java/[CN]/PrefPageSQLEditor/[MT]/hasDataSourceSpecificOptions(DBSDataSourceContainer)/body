{
  DBPPreferenceStore store=dataSourceDescriptor.getPreferenceStore();
  return store.contains(DBeaverPreferences.EDITOR_SEPARATE_CONNECTION) || store.contains(DBeaverPreferences.SCRIPT_AUTO_FOLDERS) || store.contains(DBeaverPreferences.SCRIPT_TITLE_PATTERN)|| store.contains(SQLPreferenceConstants.SQLEDITOR_CLOSE_SINGLE_QUOTES)|| store.contains(SQLPreferenceConstants.SQLEDITOR_CLOSE_DOUBLE_QUOTES)|| store.contains(SQLPreferenceConstants.SQLEDITOR_CLOSE_BRACKETS)|| store.contains(SQLPreferenceConstants.RESET_CURSOR_ON_EXECUTE);
}
