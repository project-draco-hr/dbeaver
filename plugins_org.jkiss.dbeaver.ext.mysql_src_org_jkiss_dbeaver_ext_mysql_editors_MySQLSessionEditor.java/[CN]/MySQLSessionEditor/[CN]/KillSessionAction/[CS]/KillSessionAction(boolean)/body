{
  super(killQuery ? MySQLMessages.editors_session_editor_action_terminate_Query : MySQLMessages.editors_session_editor_action_kill_Session,killQuery ? UIUtils.getShardImageDescriptor(ISharedImages.IMG_ELCL_STOP) : DBeaverIcons.getImageDescriptor(UIIcon.SQL_DISCONNECT));
  this.killQuery=killQuery;
}
