{
  DataExportSettings exportSettings=getWizard().getSettings();
  DataExporterDescriptor exporter=exportSettings.getDataExporter();
  propertySource=new PropertySourceCustom(exporter.getProperties(),exportSettings.getExtractorProperties());
  propsEditor.loadProperties(propertySource);
switch (exportSettings.getLobExtractType()) {
case SKIP:
    lobExtractType.select(EXTRACT_LOB_SKIP);
  break;
case FILES:
lobExtractType.select(EXTRACT_LOB_FILES);
break;
case INLINE:
lobExtractType.select(EXTRACT_LOB_INLINE);
break;
}
switch (exportSettings.getLobEncoding()) {
case BASE64:
lobEncodingCombo.select(LOB_ENCODING_BASE64);
break;
case HEX:
lobEncodingCombo.select(LOB_ENCODING_HEX);
break;
case BINARY:
lobEncodingCombo.select(LOB_ENCODING_BINARY);
break;
}
updatePageCompletion();
}
