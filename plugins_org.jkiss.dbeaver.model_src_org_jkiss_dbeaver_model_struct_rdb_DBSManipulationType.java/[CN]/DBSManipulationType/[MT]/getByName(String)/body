{
  if (name.toUpperCase(Locale.ENGLISH).equals(INSERT.getName())) {
    return INSERT;
  }
 else   if (name.toUpperCase(Locale.ENGLISH).equals(DELETE.getName())) {
    return DELETE;
  }
  if (name.toUpperCase(Locale.ENGLISH).equals(UPDATE.getName())) {
    return UPDATE;
  }
 else {
    return UNKNOWN;
  }
}
