{
  if (name == null) {
    return null;
  }
  name=name.trim();
  if (name.length() == 0) {
    return null;
  }
  if (underscoreSpaces) {
    name=name.replace(' ','_');
  }
  try {
    return (T)Enum.valueOf(type,name);
  }
 catch (  Exception e) {
    e.printStackTrace();
    return null;
  }
}
