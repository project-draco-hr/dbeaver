{
  AbstractPreferenceStore store=dataSourceDescriptor.getPreferenceStore();
  return store.contains(PrefConstants.RS_EDIT_MAX_TEXT_SIZE) || store.contains(PrefConstants.RS_EDIT_LONG_AS_LOB) || store.contains(PrefConstants.RS_COMMIT_ON_EDIT_APPLY)|| store.contains(PrefConstants.RS_COMMIT_ON_CONTENT_APPLY);
}
