{
  DBPPreferenceStore store=dataSourceDescriptor.getPreferenceStore();
  return store.contains(DBeaverPreferences.RESULT_SET_SHOW_ODD_ROWS) || store.contains(DBeaverPreferences.RESULT_SET_SHOW_CELL_ICONS) || store.contains(DBeaverPreferences.RESULT_SET_DOUBLE_CLICK)|| store.contains(DBeaverPreferences.RESULT_SET_AUTO_SWITCH_MODE)|| store.contains(DBeaverPreferences.RESULT_TEXT_MAX_COLUMN_SIZE);
}
