{
  if (value instanceof Number) {
    long dateValue=((Number)value).longValue();
switch (unit) {
case seconds:
      dateValue*=1000;
    break;
case nanoseconds:
  dateValue/=1000;
break;
}
return new SimpleDateFormat("yyyy-MM-dd HH:mm:ss",Locale.ENGLISH).format(new Date(dateValue));
}
return DBUtils.getDefaultValueDisplayString(value,format);
}
