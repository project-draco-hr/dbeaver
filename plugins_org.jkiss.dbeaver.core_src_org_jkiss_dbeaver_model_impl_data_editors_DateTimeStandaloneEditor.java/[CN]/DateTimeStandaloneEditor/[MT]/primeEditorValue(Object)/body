{
  if (value instanceof Date) {
    Calendar cl=Calendar.getInstance();
    cl.setTime((Date)value);
    if (dateEditor != null) {
      dateEditor.setDate(cl.get(Calendar.YEAR),cl.get(Calendar.MONTH),cl.get(Calendar.DAY_OF_MONTH));
    }
    if (timeEditor != null) {
      timeEditor.setValue(value);
    }
  }
}
