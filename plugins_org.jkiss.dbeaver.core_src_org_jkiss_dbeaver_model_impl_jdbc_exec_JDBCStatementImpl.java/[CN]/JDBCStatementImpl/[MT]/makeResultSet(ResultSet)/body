{
  if (resultSet == null) {
    return null;
  }
  JDBCResultSetImpl dbResult=createResultSetImpl(resultSet);
  if (rsOffset > 0) {
    JDBCUtils.scrollResultSet(resultSet,rsOffset);
  }
  if (rsMaxRows > 0 && connection.getDataSource().getInfo().supportsResultSetLimit()) {
    dbResult.setMaxRows(rsMaxRows);
  }
  return dbResult;
}
