{
  if (valueViewer == null) {
    return;
  }
  try {
    valueSaving=true;
    Object newValue=valueViewer.extractEditorValue();
    previewController.updateValue(newValue);
  }
 catch (  DBException e) {
    UIUtils.showErrorDialog(null,"Value save","Can't save edited value",e);
  }
 finally {
    valueSaving=false;
  }
}
