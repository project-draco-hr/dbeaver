{
  toolBarManager.add(new Separator());
  if (previewController != null && !previewController.isReadOnly()) {
    toolBarManager.add(saveCellAction);
  }
  toolBarManager.add(ActionUtils.makeCommandContribution(resultSet.getSite(),SpreadsheetCommandHandler.CMD_TOGGLE_PREVIEW,CommandContributionItem.STYLE_PUSH,UIIcon.REJECT));
}
