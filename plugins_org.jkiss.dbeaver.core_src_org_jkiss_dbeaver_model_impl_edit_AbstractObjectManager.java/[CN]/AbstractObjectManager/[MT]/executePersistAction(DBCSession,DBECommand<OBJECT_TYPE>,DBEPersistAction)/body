{
  String script=action.getScript();
  if (script == null) {
    action.handleExecute(null);
  }
 else {
    DBCStatement dbStat=DBUtils.createStatement(session,script);
    try {
      dbStat.executeStatement();
      action.handleExecute(null);
    }
 catch (    DBCException e) {
      action.handleExecute(e);
      throw e;
    }
 finally {
      dbStat.close();
    }
  }
}
