{
  String value=p != null ? p.get(name) : null;
  if (value == null) {
    return parentStore != null && useParent ? (useParentDefault ? parentStore.getDefaultLong(name) : parentStore.getLong(name)) : LONG_DEFAULT_DEFAULT;
  }
  long ival=LONG_DEFAULT_DEFAULT;
  try {
    ival=Long.parseLong(value);
  }
 catch (  NumberFormatException e) {
  }
  return ival;
}
