{
  String value=p != null ? p.get(name) : null;
  if (value == null) {
    return parentStore != null && useParent ? (useParentDefault ? parentStore.getDefaultDouble(name) : parentStore.getDouble(name)) : DOUBLE_DEFAULT_DEFAULT;
  }
  double ival=DOUBLE_DEFAULT_DEFAULT;
  try {
    ival=Double.parseDouble(value);
  }
 catch (  NumberFormatException e) {
  }
  return ival;
}
