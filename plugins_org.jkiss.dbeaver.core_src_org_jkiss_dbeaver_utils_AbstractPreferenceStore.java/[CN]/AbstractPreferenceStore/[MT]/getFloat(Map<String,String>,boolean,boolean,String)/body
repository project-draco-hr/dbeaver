{
  String value=p != null ? p.get(name) : null;
  if (value == null) {
    return parentStore != null && useParent ? (useParentDefault ? parentStore.getDefaultFloat(name) : parentStore.getFloat(name)) : FLOAT_DEFAULT_DEFAULT;
  }
  float ival=FLOAT_DEFAULT_DEFAULT;
  try {
    ival=Float.parseFloat(value);
  }
 catch (  NumberFormatException e) {
  }
  return ival;
}
