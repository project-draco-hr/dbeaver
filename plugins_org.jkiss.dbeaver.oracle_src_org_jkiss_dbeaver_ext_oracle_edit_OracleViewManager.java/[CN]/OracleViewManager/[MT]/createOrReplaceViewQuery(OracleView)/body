{
  StringBuilder decl=new StringBuilder(200);
  final String lineSeparator=ContentUtils.getDefaultLineSeparator();
  decl.append("CREATE OR REPLACE VIEW ").append(view.getFullQualifiedName()).append(lineSeparator).append("AS ").append(view.getAdditionalInfo().getText());
  return new IDatabasePersistAction[]{new AbstractDatabasePersistAction(OracleMessages.edit_oracle_view_manager_action_create_view,decl.toString())};
}
