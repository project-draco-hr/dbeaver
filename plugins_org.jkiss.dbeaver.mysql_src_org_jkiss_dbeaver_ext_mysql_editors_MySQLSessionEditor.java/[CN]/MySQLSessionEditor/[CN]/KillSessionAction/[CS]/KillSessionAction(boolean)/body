{
  super(killQuery ? MySQLMessages.editors_session_editor_action_terminate_Query : MySQLMessages.editors_session_editor_action_kill_Session,killQuery ? PlatformUI.getWorkbench().getSharedImages().getImageDescriptor(ISharedImages.IMG_ELCL_STOP) : DBIcon.SQL_DISCONNECT.getImageDescriptor());
  this.killQuery=killQuery;
}
