{
  addResolver(new SQLEntityResolver());
  addResolver(new SQLContainerResolver<DBSSchema>(SQLContainerResolver.VAR_NAME_SCHEMA,"Schema",DBSSchema.class));
  addResolver(new SQLContainerResolver<DBSCatalog>(SQLContainerResolver.VAR_NAME_CATALOG,"Catalog",DBSCatalog.class));
  addResolver(new SQLAttributeResolver());
}
