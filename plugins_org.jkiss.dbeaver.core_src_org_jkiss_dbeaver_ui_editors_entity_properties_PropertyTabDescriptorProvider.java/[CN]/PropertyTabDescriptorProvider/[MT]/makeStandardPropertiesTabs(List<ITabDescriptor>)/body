{
  tabList.add(new PropertyTabDescriptor(PropertiesContributor.CATEGORY_INFO,PropertiesContributor.TAB_STANDARD,CoreMessages.ui_properties_category_information,DBIcon.TREE_INFO.getImage(),new SectionDescriptor(PropertiesContributor.SECTION_STANDARD,PropertiesContributor.TAB_STANDARD){
