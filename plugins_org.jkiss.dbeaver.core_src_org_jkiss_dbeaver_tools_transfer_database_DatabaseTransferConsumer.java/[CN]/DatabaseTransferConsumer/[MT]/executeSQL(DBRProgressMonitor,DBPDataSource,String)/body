{
  DBCSession session=dataSource.openSession(monitor,DBCExecutionPurpose.UTIL,"Create target metadata");
  try {
    DBCStatement dbStat=DBUtils.prepareStatement(session,sql,false);
    try {
      dbStat.executeStatement();
    }
  finally {
      dbStat.close();
    }
  }
  finally {
    session.close();
  }
}
