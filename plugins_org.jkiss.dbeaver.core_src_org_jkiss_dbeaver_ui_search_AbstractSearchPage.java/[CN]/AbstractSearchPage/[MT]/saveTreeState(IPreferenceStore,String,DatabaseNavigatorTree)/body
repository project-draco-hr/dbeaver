{
  StringBuilder sourcesString=new StringBuilder();
  Object[] nodes=tree.getViewer() instanceof CheckboxTreeViewer ? ((CheckboxTreeViewer)tree.getViewer()).getCheckedElements() : ((IStructuredSelection)tree.getViewer().getSelection()).toArray();
  for (  Object obj : nodes) {
    DBNNode node=(DBNNode)obj;
    if (sourcesString.length() > 0) {
      sourcesString.append("|");
    }
    sourcesString.append(node.getNodeItemPath());
  }
  store.setValue(propName,sourcesString.toString());
}
