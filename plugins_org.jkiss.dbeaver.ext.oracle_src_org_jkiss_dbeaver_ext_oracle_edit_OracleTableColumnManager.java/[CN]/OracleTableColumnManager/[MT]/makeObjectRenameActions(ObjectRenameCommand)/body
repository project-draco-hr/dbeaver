{
  final OracleTableColumn column=command.getObject();
  return new DBEPersistAction[]{new SQLDatabasePersistAction("Rename column","ALTER TABLE " + column.getTable().getFullQualifiedName() + " RENAME COLUMN "+ DBUtils.getQuotedIdentifier(column.getDataSource(),command.getOldName())+ " TO "+ command.getNewName())};
}
