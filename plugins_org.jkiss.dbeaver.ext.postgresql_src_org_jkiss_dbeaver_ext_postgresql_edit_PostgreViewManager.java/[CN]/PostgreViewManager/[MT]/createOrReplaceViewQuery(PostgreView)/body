{
  StringBuilder decl=new StringBuilder(200);
  final String lineSeparator=GeneralUtils.getDefaultLineSeparator();
  decl.append("CREATE OR REPLACE VIEW ").append(view.getFullQualifiedName()).append(lineSeparator).append("AS ").append(view.getSource());
  return new DBEPersistAction[]{new SQLDatabasePersistAction("Create view",decl.toString())};
}
