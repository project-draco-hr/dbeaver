{
  if (textEdit != null) {
    return JDBCNumberValueHandler.convertStringToNumber(textEdit.getText(),value,getValueController().getAttributeMetaData());
  }
 else   if (bitEdit != null) {
    if (isBoolean) {
      return bitEdit.getSelectionIndex() == 0 ? Boolean.FALSE : Boolean.TRUE;
    }
 else {
      return (byte)bitEdit.getSelectionIndex();
    }
  }
 else {
    return null;
  }
}
