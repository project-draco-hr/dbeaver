{
  if (tableColumn != null) {
    return tableColumn;
  }
  if (tableMetaData == null) {
    return null;
  }
  DBSEntity entity=tableMetaData.getEntity(monitor);
  if (entity == null) {
    return null;
  }
  if (pseudoAttribute != null) {
    tableColumn=pseudoAttribute.createFakeAttribute(entity,this);
  }
 else {
    tableColumn=entity.getAttribute(monitor,name);
  }
  return tableColumn;
}
