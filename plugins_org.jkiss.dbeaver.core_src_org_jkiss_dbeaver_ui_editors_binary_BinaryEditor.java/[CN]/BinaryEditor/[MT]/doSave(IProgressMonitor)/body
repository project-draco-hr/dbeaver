{
  IEditorInput editorInput=getEditorInput();
  if (editorInput instanceof IPathEditorInput) {
    final IPath absolutePath=Platform.getLocation().append(((IPathEditorInput)editorInput).getPath());
    File systemFile=absolutePath.toFile();
    try {
      manager.getContent().get(systemFile);
    }
 catch (    IOException e) {
      log.error("Could not save binary content",e);
    }
    IFile file=(IFile)editorInput.getAdapter(IFile.class);
    if (file != null) {
      ContentUtils.syncFile(RuntimeUtils.makeMonitor(monitor),file);
    }
  }
}
