{
  if (page == pageDrivers) {
    return getPageSettings(pageDrivers.getSelectedDriver());
  }
 else   if (page instanceof ConnectionPageSettings) {
    if (pageDrivers.getSelectedDriver().isEmbedded()) {
      return pageGeneral;
    }
 else {
      return pageNetwork;
    }
  }
 else   if (page instanceof ConnectionPageNetwork) {
    return pageGeneral;
  }
 else {
    return null;
  }
}
