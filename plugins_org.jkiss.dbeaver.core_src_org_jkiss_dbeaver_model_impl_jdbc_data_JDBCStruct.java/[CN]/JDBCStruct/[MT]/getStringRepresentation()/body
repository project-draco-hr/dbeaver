{
  String str=stringRepresentation != null ? stringRepresentation.get() : null;
  if (str == null) {
    try {
      str=makeStructString();
      stringRepresentation=new SoftReference<String>(str);
    }
 catch (    SQLException e) {
      log.error(e);
      return contents.toString();
    }
  }
  return str;
}
