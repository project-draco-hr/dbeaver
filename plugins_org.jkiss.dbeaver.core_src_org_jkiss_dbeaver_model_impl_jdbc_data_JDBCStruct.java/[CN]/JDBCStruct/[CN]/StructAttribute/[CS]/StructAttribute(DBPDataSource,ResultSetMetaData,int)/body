{
  super(metaData.getColumnName(index),metaData.getColumnTypeName(index),metaData.getColumnType(index),index,metaData.getColumnDisplaySize(index),metaData.getScale(index),metaData.getPrecision(index),metaData.isNullable(index) == ResultSetMetaData.columnNoNulls,metaData.isAutoIncrement(index));
  dataKind=JDBCUtils.resolveDataKind(dataSource,getTypeName(),getTypeID());
}
