{
  IPreferenceStore store=isDataSourcePreferencePage() ? getDataSourceContainer().getPreferenceStore() : DBeaverCore.getGlobalPreferenceStore();
  if (isDataSourcePreferencePage() && !useDataSourceSettings()) {
    clearPreferences(store);
  }
 else {
    savePreferences(store);
  }
  RuntimeUtils.savePreferenceStore(store);
  return super.performOk();
}
