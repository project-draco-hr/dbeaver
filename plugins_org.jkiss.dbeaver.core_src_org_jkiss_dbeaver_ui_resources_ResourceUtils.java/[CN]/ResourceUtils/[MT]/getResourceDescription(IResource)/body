{
  if (resource instanceof IFolder) {
    return "";
  }
 else   if (SCRIPT_FILE_EXTENSION.equals(resource.getFileExtension())) {
    String description=SQLUtils.getScriptDescription((IFile)resource);
    if (CommonUtils.isEmptyTrimmed(description)) {
      description="<empty>";
    }
    return description;
  }
 else {
    return "";
  }
}
