{
  JarInputStream jis=new JarInputStream(is);
  JarOutputStream jos=null;
  Manifest manifest=jis.getManifest();
  if (manifest == null) {
    jos=new JarOutputStream(os);
  }
 else {
    manifest.getEntries().clear();
    jos=new JarOutputStream(os,manifest);
  }
  JarEntry src;
  while ((src=jis.getNextJarEntry()) != null) {
    String name=src.getName();
    String lowercaseName=name.toLowerCase();
    if (lowercaseName.startsWith("meta-inf") && (lowercaseName.endsWith(".rsa") || lowercaseName.endsWith(".dsa") || lowercaseName.endsWith(".sf"))) {
    }
 else {
      JarEntry dest=new JarEntry(name);
      jos.putNextEntry(dest);
      copyContent(jis,jos);
    }
  }
  jos.close();
  jis.close();
}
