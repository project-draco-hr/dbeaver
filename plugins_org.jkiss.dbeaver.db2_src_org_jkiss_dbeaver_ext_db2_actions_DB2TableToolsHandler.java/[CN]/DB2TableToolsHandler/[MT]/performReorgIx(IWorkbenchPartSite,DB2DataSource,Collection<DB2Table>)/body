{
  DB2TableReorgIndexDialog2 dialog=new DB2TableReorgIndexDialog2(partSite,dataSource,tables);
  dialog.open();
  dialog.setOnSuccess(new Runnable(){
    @Override public void run(){
      UIUtils.showMessageBox(partSite.getShell(),DB2Messages.dialog_table_tools_success_title,DB2Messages.dialog_table_tools_reorgix_success,SWT.ICON_INFORMATION);
    }
  }
);
}
