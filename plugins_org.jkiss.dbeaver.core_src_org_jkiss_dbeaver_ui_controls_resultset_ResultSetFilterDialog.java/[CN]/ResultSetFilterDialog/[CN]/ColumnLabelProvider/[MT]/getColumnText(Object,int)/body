{
  DBDAttributeConstraint constraint=(DBDAttributeConstraint)element;
switch (columnIndex) {
case 0:
    return constraint.getAttribute().getName();
case 1:
  return String.valueOf(constraint.getOriginalVisualPosition() + 1);
case 2:
{
  int orderPosition=constraint.getOrderPosition();
  if (orderPosition > 0) {
    return String.valueOf(orderPosition);
  }
  return "";
}
case 3:
{
String condition=SQLUtils.getConstraintCondition(resultSetViewer.getDataSource(),constraint,true);
if (condition != null) {
  return condition;
}
 else {
  return "";
}
}
default :
return "";
}
}
