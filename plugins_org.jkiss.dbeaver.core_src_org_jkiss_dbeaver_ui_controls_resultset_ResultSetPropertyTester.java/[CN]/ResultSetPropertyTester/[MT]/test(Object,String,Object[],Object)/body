{
  Spreadsheet spreadsheet=Spreadsheet.getFromGrid((LightGrid)receiver);
  if (spreadsheet == null) {
    return false;
  }
  ResultSetViewer rsv=(ResultSetViewer)spreadsheet.getController();
  if (rsv != null) {
    return checkResultSetProperty(rsv,property,expectedValue);
  }
 else {
    return false;
  }
}
