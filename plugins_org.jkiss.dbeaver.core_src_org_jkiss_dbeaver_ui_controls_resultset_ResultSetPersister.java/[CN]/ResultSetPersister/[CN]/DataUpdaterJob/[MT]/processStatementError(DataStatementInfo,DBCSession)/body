{
  statement.executed=false;
  DBCTransactionManager txnManager=DBUtils.getTransactionManager(getExecutionContext());
  if (txnManager != null) {
    try {
      txnManager.rollback(session,savepoint);
    }
 catch (    Throwable e) {
      log.debug("Error during transaction rollback",e);
    }
  }
}
