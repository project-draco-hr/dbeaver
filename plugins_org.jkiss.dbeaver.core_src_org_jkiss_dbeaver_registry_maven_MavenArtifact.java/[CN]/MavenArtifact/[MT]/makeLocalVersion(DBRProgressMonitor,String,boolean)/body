{
  MavenArtifactVersion version=getVersion(versionStr);
  if (version == null) {
    version=new MavenArtifactVersion(monitor,this,versionStr);
    localVersions.add(version);
    repository.flushCache();
  }
  if (setActive) {
    activeVersion=versionStr;
    repository.flushCache();
  }
  return version;
}
