{
  return new IDatabasePersistAction[]{new AbstractDatabasePersistAction(MySQLMessages.edit_user_manager_command_drop_user,"DROP USER " + getObject().getFullName()){
    @Override public void handleExecute(    Throwable error){
      if (error == null) {
        getObject().setPersisted(false);
      }
    }
  }
};
}
