{
  RuntimeUtils.setDefaultPreferenceValue(store,QMConstants.PROP_HISTORY_DAYS,90);
  RuntimeUtils.setDefaultPreferenceValue(store,QMConstants.PROP_ENTRIES_PER_PAGE,200);
