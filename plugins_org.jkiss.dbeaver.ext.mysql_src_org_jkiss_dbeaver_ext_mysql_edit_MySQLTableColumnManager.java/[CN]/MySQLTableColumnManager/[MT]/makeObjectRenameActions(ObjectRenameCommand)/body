{
  final MySQLTableColumn column=command.getObject();
  return new DBEPersistAction[]{new SQLDatabasePersistAction("Rename column","ALTER TABLE " + column.getTable().getFullQualifiedName() + " CHANGE "+ DBUtils.getQuotedIdentifier(column.getDataSource(),command.getOldName())+ " "+ getNestedDeclaration(column.getTable(),command))};
}
