{
  final int result=ConfirmationDialog.showConfirmDialog(getSite().getShell(),PrefConstants.CONFIRM_ENTITY_EDIT_CLOSE,ConfirmationDialog.QUESTION_WITH_CANCEL,getEditorInput().getTreeNode().getNodeName());
  if (result == IDialogConstants.YES_ID) {
    return ISaveablePart2.YES;
  }
 else   if (result == IDialogConstants.NO_ID) {
    return ISaveablePart2.NO;
  }
 else {
    return ISaveablePart2.CANCEL;
  }
}
