{
  ViewSQLDialog dialog=new ViewSQLDialog(getEditorSite(),getDataSource().getContainer(),allowSave ? CoreMessages.editors_entity_dialog_persist_title : CoreMessages.editors_entity_dialog_preview_title,script.toString());
  dialog.setShowSaveButton(allowSave);
  dialog.setImage(DBIcon.SQL_PREVIEW.getImage());
  result=dialog.open();
}
