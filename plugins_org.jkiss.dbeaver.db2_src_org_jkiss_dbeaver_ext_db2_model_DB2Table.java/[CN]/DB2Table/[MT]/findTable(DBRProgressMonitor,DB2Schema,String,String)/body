{
  if (schema == null) {
    log.debug("Referenced schema '" + ownerName + "' not found");
    return null;
  }
  DB2Table table=schema.getTableCache().getObject(monitor,schema,tableName);
  if (table == null) {
    log.debug("Table '" + tableName + "' not found in schema '"+ ownerName+ "'");
  }
  return table;
}
