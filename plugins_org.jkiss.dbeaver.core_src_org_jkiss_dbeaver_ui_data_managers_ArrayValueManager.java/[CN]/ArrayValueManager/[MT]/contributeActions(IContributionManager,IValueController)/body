{
  manager.add(new Action("Add element",DBeaverIcons.getImageDescriptor(UIIcon.ROW_ADD)){
    @Override public void run(){
      super.run();
    }
  }
);
  manager.add(new Action("Remove element",DBeaverIcons.getImageDescriptor(UIIcon.ROW_DELETE)){
    @Override public void run(){
      super.run();
    }
  }
);
}
