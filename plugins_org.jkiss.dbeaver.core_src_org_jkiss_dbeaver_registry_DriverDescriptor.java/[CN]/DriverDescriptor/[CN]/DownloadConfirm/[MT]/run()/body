{
  DriverDownloadDialog downloadDialog=new DriverDownloadDialog(DBeaverUI.getActiveWorkbenchWindow(),DriverDescriptor.this);
  downloadDialog.open();
  proceed=ConfirmationDialog.showConfirmDialog(null,DBeaverPreferences.CONFIRM_DRIVER_DOWNLOAD,ConfirmationDialog.QUESTION,getFullName(),libNames) == IDialogConstants.YES_ID;
}
