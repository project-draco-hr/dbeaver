{
  if (valueEditor == null) {
    return;
  }
  try {
    valueSaving=true;
    Object newValue=valueEditor.extractEditorValue();
    previewController.updateValue(newValue);
    presentation.refreshData(false,false);
  }
 catch (  Exception e) {
    UIUtils.showErrorDialog(null,"Value save","Can't save edited value",e);
  }
 finally {
    valueSaving=false;
  }
}
