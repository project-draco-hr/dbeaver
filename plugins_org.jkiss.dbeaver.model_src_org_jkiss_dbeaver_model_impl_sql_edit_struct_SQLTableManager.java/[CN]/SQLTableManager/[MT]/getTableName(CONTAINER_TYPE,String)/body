{
  for (int i=0; ; i++) {
    String tableName=DBObjectNameCaseTransformer.transformName(container.getDataSource(),i == 0 ? baseName : (baseName + "_" + i));
    DBSObject child=container.getChild(VoidProgressMonitor.INSTANCE,tableName);
    if (child == null) {
      return tableName;
    }
  }
}
