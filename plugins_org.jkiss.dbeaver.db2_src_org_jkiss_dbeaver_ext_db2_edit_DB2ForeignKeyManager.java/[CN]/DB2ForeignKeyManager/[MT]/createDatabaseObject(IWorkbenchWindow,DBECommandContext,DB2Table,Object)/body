{
  EditForeignKeyDialog editDialog=new EditForeignKeyDialog(workbenchWindow.getShell(),DB2Messages.edit_db2_foreign_key_manager_dialog_title,table,new DBSForeignKeyModifyRule[]{DBSForeignKeyModifyRule.NO_ACTION,DBSForeignKeyModifyRule.CASCADE,DBSForeignKeyModifyRule.RESTRICT,DBSForeignKeyModifyRule.SET_NULL,DBSForeignKeyModifyRule.SET_DEFAULT});
  if (editDialog.open() != IDialogConstants.OK_ID) {
    return null;
  }
  return null;
}
