{
  try {
    opened=true;
    int result=super.open();
    if (result == IDialogConstants.OK_ID) {
      getValueController().updateValue(editedValue);
    }
    return result;
  }
  finally {
    dialogCount--;
    if (this.valueController != null) {
      this.valueController.unregisterEditor(this);
      this.valueController=null;
    }
  }
}
