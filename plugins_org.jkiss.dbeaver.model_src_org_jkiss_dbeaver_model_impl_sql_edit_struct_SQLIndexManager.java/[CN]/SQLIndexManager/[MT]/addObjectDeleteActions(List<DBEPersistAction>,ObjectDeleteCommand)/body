{
  actions.add(new SQLDatabasePersistAction(ModelMessages.model_jdbc_drop_index,getDropIndexPattern(command.getObject()).replace(PATTERN_ITEM_TABLE,command.getObject().getTable().getFullyQualifiedName(DBPEvaluationContext.DDL)).replace(PATTERN_ITEM_INDEX,command.getObject().getFullyQualifiedName(DBPEvaluationContext.DDL)).replace(PATTERN_ITEM_INDEX_SHORT,command.getObject().getName())));
}
