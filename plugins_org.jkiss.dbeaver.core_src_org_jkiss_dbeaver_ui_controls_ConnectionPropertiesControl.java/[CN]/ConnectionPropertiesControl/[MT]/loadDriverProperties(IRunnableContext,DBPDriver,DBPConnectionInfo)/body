{
  try {
    final IPropertyDescriptor[] connectionsProps=driver.getDataSourceProvider().getConnectionProperties(runnableContext,driver,connectionInfo);
    driverProvidedProperties=new ArrayList<IPropertyDescriptor>();
    if (connectionsProps != null) {
      Collections.addAll(driverProvidedProperties,connectionsProps);
    }
  }
 catch (  DBException e) {
    log.warn("Can't load driver properties",e);
  }
}
