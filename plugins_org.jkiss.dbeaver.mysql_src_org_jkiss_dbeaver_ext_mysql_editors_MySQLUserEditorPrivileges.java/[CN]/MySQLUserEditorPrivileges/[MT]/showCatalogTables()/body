{
  LoadingUtils.createService(new DatabaseLoadService<Collection<MySQLTable>>(MySQLMessages.editors_user_editor_privileges_service_load_tables,getDataSource()){
    @Override public Collection<MySQLTable> evaluate() throws InvocationTargetException, InterruptedException {
      if (selectedCatalog == null) {
        return Collections.emptyList();
      }
      try {
        return selectedCatalog.getTables(getProgressMonitor());
      }
 catch (      DBException e) {
        log.error(e);
      }
      return null;
    }
  }
,pageControl.createTablesLoadVisualizer()).schedule();
}
