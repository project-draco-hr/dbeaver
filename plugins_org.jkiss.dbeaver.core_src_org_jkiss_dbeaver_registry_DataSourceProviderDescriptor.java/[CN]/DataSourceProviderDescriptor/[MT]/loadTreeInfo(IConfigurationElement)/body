{
  DBXTreeItem treeRoot=new DBXTreeItem(this,null,config.getAttribute(RegistryConstants.ATTR_ID),CoreMessages.model_navigator_Connection,CoreMessages.model_navigator_Connection,config.getAttribute(RegistryConstants.ATTR_PATH),null,false,true,false,false,false,config.getAttribute(RegistryConstants.ATTR_VISIBLE_IF),null);
