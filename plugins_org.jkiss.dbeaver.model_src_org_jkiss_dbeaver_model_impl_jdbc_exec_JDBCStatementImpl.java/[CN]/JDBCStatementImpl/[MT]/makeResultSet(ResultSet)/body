{
  if (resultSet == null) {
    return null;
  }
  JDBCResultSet dbResult=createResultSetImpl(resultSet);
  if (rsOffset > 0) {
    JDBCUtils.scrollResultSet(resultSet,rsOffset,!getConnection().getDataSource().getInfo().supportsResultSetScroll());
  }
  if (rsMaxRows > 0) {
    dbResult.setMaxRows(rsMaxRows);
  }
  return dbResult;
}
