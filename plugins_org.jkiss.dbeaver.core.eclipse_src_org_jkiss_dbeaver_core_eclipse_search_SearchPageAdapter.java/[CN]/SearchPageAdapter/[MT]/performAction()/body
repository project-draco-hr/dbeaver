{
  try {
    source.saveState(DBeaverCore.getGlobalPreferenceStore());
    NewSearchUI.runQueryInBackground(createQueryAdapter(source.createQuery()));
  }
 catch (  Exception e) {
    UIUtils.showErrorDialog(getControl().getShell(),"Search","Can't perform search",e);
    return false;
  }
  return true;
}
