{
  if (required == IFindReplaceTarget.class) {
    return findReplaceTarget;
  }
  ResultSetViewer resultsView=getActiveResultSetViewer();
  if (resultsView != null) {
    Object adapter=resultsView.getAdapter(required);
    if (adapter != null) {
      return adapter;
    }
  }
  return super.getAdapter(required);
}
