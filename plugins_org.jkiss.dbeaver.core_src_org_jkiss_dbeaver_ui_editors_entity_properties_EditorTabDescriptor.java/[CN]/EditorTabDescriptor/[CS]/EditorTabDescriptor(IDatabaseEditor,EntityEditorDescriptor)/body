{
  this.mainEditor=part;
  this.descriptor=descriptor;
  setSectionDescriptors(Collections.singletonList(new SectionDescriptor(PropertiesContributor.SECTION_STANDARD,this.descriptor.getId()){
    @Override public ISection getSectionClass(){
      return new EditorWrapperSection(EditorTabDescriptor.this.mainEditor,EditorTabDescriptor.this.descriptor);
    }
  }
));
}
