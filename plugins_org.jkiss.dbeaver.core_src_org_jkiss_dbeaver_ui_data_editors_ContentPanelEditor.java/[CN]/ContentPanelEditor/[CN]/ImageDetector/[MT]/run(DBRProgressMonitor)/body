{
  if (!content.isNull()) {
    try {
      InputStream contentStream=content.getContents(monitor).getContentStream();
      try {
        new ImageData(contentStream);
      }
  finally {
        ContentUtils.close(contentStream);
      }
      isImage=true;
    }
 catch (    Exception e) {
      log.debug(e.getMessage());
    }
  }
}
