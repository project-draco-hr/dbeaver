{
  List<NetworkHandlerDescriptor> result=new ArrayList<>();
  for (  NetworkHandlerDescriptor d : descriptors) {
    try {
      if (!d.hasObjectTypes() || d.matches(dataSource.getDriver().getDataSourceProvider())) {
        result.add(d);
      }
    }
 catch (    DBException e) {
    }
  }
  return result;
}
