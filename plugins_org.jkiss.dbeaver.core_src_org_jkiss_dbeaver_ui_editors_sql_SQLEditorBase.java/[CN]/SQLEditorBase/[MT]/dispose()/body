{
  if (decorationSupport != null) {
    decorationSupport.dispose();
    decorationSupport=null;
  }
  syntaxManager.dispose();
  if (themeListener != null) {
    PlatformUI.getWorkbench().getThemeManager().removePropertyChangeListener(themeListener);
    themeListener=null;
  }
  super.dispose();
}
