{
  OverviewRuler overviewRuler=null;
  if (hasAnnotations()) {
    overviewRuler=new OverviewRuler(getAnnotationAccess(),VERTICAL_RULER_WIDTH,getSharedColors());
  }
  SQLEditorSourceViewer sourceViewer=createSourceViewer(parent,ruler,styles,overviewRuler);
  getSourceViewerDecorationSupport(sourceViewer);
  return sourceViewer;
}
