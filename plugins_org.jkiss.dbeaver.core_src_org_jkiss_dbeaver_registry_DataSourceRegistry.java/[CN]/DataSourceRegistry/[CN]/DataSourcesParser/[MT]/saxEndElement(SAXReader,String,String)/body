{
  if (localName.equals(RegistryConstants.TAG_DATA_SOURCE)) {
    curDataSource=null;
  }
 else   if (localName.equals(RegistryConstants.TAG_NETWORK_HANDLER)) {
    curNetworkHandler=null;
  }
 else   if (localName.equals(RegistryConstants.TAG_FILTER)) {
    curFilter=null;
  }
  isDescription=false;
}
