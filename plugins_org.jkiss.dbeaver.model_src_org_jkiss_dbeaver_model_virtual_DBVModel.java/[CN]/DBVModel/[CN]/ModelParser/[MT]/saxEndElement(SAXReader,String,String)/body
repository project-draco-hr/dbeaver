{
  if (localName.equals(TAG_CONTAINER)) {
    curContainer=curContainer.getParentObject();
  }
 else   if (localName.equals(TAG_ENTITY)) {
    curEntity=null;
  }
 else   if (localName.equals(TAG_CONSTRAINT)) {
    curConstraint=null;
  }
 else   if (localName.equals(TAG_ATTRIBUTE)) {
    curAttribute=null;
  }
}
