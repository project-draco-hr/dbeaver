{
  this.name=JDBCUtils.safeGetString(dbResult,MSSQLConstants.COL_ENGINE_NAME);
  this.description=JDBCUtils.safeGetString(dbResult,MSSQLConstants.COL_ENGINE_DESCRIPTION);
  this.support=Support.valueOf(JDBCUtils.safeGetString(dbResult,MSSQLConstants.COL_ENGINE_SUPPORT));
  this.supportsTransactions="YES".equals(JDBCUtils.safeGetString(dbResult,MSSQLConstants.COL_ENGINE_SUPPORT_TXN));
  this.supportsXA="YES".equals(JDBCUtils.safeGetString(dbResult,MSSQLConstants.COL_ENGINE_SUPPORT_XA));
  this.supportsSavepoints="YES".equals(JDBCUtils.safeGetString(dbResult,MSSQLConstants.COL_ENGINE_SUPPORT_SAVEPOINTS));
}
