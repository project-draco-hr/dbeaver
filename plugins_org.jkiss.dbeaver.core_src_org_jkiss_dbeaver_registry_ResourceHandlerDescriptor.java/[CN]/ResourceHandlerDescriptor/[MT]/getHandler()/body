{
  if (handler == null) {
    Class<DBPResourceHandler> clazz=handlerType.getObjectClass(DBPResourceHandler.class);
    if (clazz == null) {
      return null;
    }
    try {
      handler=clazz.newInstance();
    }
 catch (    Exception e) {
      log.error("Can't instantiate resource handler",e);
    }
  }
  return handler;
}
