{
  if (isQMLoggingEnabled()) {
    if (o != null && !DBUtils.isAtomicParameter(o)) {
      o=new ContentParameter(o);
    }
    if (paramMap == null) {
      paramMap=new LinkedHashMap<>();
    }
    paramMap.put(parameter,o);
  }
  QMUtils.getDefaultHandler().handleStatementBind(this,parameter,o);
}
