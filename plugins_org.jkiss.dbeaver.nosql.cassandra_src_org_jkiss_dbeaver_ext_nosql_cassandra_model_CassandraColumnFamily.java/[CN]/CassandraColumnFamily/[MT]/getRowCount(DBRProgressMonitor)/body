{
  if (rowCount != null) {
    return rowCount;
  }
  DBCExecutionContext context=getDataSource().openContext(monitor,DBCExecutionPurpose.META,"Read row count");
  try {
    rowCount=countData(context,null);
  }
 catch (  DBException e) {
    log.debug("Can't fetch row count: " + e.getMessage());
  }
 finally {
    context.close();
  }
  if (rowCount == null) {
    rowCount=-1L;
  }
  return rowCount;
}
