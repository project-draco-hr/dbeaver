{
  MySQLEnumValue enumValue=(MySQLEnumValue)value;
  if (enumValue.isNull()) {
    control.setSelection(-1);
  }
  int itemCount=control.getItemCount();
  for (int i=0; i < itemCount; i++) {
    if (control.getItem(i).equals(enumValue.getValue())) {
      control.setSelection(i);
      break;
    }
  }
}
