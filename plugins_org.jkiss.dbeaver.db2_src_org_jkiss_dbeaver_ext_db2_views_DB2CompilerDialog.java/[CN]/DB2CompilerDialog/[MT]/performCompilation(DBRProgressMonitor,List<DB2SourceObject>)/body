{
  compileLog.layoutLog();
  for (  DB2SourceObject unit : units) {
    if (monitor.isCanceled()) {
      break;
    }
    final String message=NLS.bind(DB2Messages.views_db2_compiler_dialog_message_compile_unit,unit.getSourceType().name(),unit.getName());
    compileLog.info(message);
    boolean success=false;
    try {
      success=CompileHandler.compileUnit(monitor,compileLog,unit);
    }
 catch (    DBCException e) {
      log.error("Compile error",e);
    }
    compileLog.info(!success ? DB2Messages.views_db2_compiler_dialog_message_compilation_error : DB2Messages.views_db2_compiler_dialog_message_compilation_success);
    compileLog.info("");
  }
}
