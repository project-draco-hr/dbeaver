{
  CommandContributionItem syncItem=new CommandContributionItem(new CommandContributionItemParameter(workbenchWindow,"org.jkiss.dbeaver.ui.editors.sql.sync",CoreCommands.CMD_SYNC_CONNECTION,CommandContributionItem.STYLE_PULLDOWN));
  ToolBarManager syncToolbar=new ToolBarManager(SWT.NONE);
