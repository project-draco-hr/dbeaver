{
  JDBCSession session=datasource.openSession(monitor,DBCExecutionPurpose.META,"Load source code");
  try {
    JDBCPreparedStatement dbStat=session.prepareStatement(sqlStatement);
    try {
      JDBCResultSet dbResult=dbStat.executeQuery();
      try {
        StringBuilder sbResult=new StringBuilder();
        while (dbResult.nextRow()) {
          sbResult.append(dbResult.getString(1).trim());
        }
        return sbResult.toString();
      }
  finally {
        dbResult.close();
      }
    }
  finally {
      dbStat.close();
    }
  }
 catch (  SQLException e) {
    throw new DBException("Can't read source code of '" + dbObjectName + "'",e);
  }
catch (  Exception e) {
    log.debug(e);
    return null;
  }
}
