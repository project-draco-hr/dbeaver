{
  EditConstraintDialog editDialog=new EditConstraintDialog(workbenchWindow.getShell(),OracleMessages.edit_oracle_constraint_manager_dialog_title,parent,new DBSEntityConstraintType[]{DBSEntityConstraintType.PRIMARY_KEY,DBSEntityConstraintType.UNIQUE_KEY});
  if (editDialog.open() != IDialogConstants.OK_ID) {
    return null;
  }
  final OracleTableConstraint constraint=new OracleTableConstraint(parent,null,editDialog.getConstraintType(),null,OracleObjectStatus.ENABLED);
  constraint.setName(DBObjectNameCaseTransformer.transformName(constraint,CommonUtils.escapeIdentifier(parent.getName()) + "_PK"));
  int colIndex=1;
  for (  DBSEntityAttribute tableColumn : editDialog.getSelectedColumns()) {
    constraint.addColumn(new OracleTableConstraintColumn(constraint,(OracleTableColumn)tableColumn,colIndex++));
  }
  return constraint;
}
