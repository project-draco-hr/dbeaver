{
  DBPPreferenceStore store=dataSourceDescriptor.getPreferenceStore();
  return store.contains(DBeaverPreferences.STATEMENT_INVALIDATE_BEFORE_EXECUTE) || store.contains(DBeaverPreferences.STATEMENT_TIMEOUT) || store.contains(DBeaverPreferences.SCRIPT_COMMIT_TYPE)|| store.contains(DBeaverPreferences.SCRIPT_ERROR_HANDLING)|| store.contains(DBeaverPreferences.SCRIPT_COMMIT_LINES)|| store.contains(DBeaverPreferences.SCRIPT_FETCH_RESULT_SETS)|| store.contains(DBeaverPreferences.SCRIPT_STATEMENT_DELIMITER)|| store.contains(DBeaverPreferences.SCRIPT_IGNORE_NATIVE_DELIMITER)|| store.contains(DBeaverPreferences.SQL_PARAMETERS_ENABLED);
}
