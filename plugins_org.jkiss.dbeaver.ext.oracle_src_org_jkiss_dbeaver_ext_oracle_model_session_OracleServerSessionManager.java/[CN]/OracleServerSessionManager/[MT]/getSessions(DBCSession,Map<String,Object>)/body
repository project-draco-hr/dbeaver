{
  try {
    JDBCPreparedStatement dbStat=((JDBCSession)session).prepareStatement("SELECT s.*,sq.SQL_TEXT FROM V$SESSION s\n" + "LEFT OUTER JOIN V$SQL sq ON sq.SQL_ID=s.SQL_ID\n" + "WHERE s.TYPE='USER'");
    try {
      JDBCResultSet dbResult=dbStat.executeQuery();
      try {
        List<OracleServerSession> sessions=new ArrayList<OracleServerSession>();
        while (dbResult.next()) {
          sessions.add(new OracleServerSession(dbResult));
        }
        return sessions;
      }
  finally {
        dbResult.close();
      }
    }
  finally {
      dbStat.close();
    }
  }
 catch (  SQLException e) {
    throw new DBException(e,session.getDataSource());
  }
}
