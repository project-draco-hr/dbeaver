{
  return attr1.getOrdinalPosition() == attr2.getOrdinalPosition() && attr1.isRequired() == attr2.isRequired() && attr1.getMaxLength() == attr2.getMaxLength() && CommonUtils.equalObjects(attr1.getName(),attr2.getName()) && attr1.getPrecision() == attr2.getPrecision() && attr1.getScale() == attr2.getScale() && attr1.getTypeID() == attr2.getTypeID() && CommonUtils.equalObjects(attr1.getTypeName(),attr2.getTypeName()) && attr1.isRequired() == attr2.isRequired() && attr1.isAutoGenerated() == attr2.isAutoGenerated() && attr1.isPseudoAttribute() == attr2.isPseudoAttribute();
}
