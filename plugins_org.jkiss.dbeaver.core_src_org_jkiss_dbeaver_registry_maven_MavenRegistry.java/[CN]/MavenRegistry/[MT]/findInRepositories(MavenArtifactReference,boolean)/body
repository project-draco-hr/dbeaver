{
  for (  MavenRepository repository : repositories) {
    MavenArtifact artifact=repository.findArtifact(ref,resolve);
    if (artifact != null) {
      return artifact;
    }
  }
  MavenArtifact artifact=localRepository.findArtifact(ref,resolve);
  if (artifact != null) {
    return artifact;
  }
  return null;
}
