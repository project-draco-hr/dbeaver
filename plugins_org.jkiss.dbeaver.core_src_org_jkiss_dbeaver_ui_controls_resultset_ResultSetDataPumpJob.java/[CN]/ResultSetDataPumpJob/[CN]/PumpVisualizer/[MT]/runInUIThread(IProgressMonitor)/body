{
  if (!progressControl.isDisposed()) {
    if (!finished) {
      try {
        showProgress(progressControl);
      }
 catch (      Exception e) {
        log.error("Internal error during progress visualization",e);
        finishProgress(progressControl);
      }
    }
 else {
      finishProgress(progressControl);
    }
  }
  return Status.OK_STATUS;
}
