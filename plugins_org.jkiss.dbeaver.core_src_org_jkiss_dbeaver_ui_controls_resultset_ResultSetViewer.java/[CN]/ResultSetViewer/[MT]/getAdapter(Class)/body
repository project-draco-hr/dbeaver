{
  if (adapter == IPropertySheetPage.class) {
    PropertyPageStandard page=new PropertyPageStandard();
    page.setPropertySourceProvider(new IPropertySourceProvider(){
      @Nullable @Override public IPropertySource getPropertySource(      Object object){
        if (object instanceof GridPos) {
          final GridPos cell=translateVisualPos((GridPos)object);
          if (isValidCell(cell)) {
            final ResultSetValueController valueController=new ResultSetValueController(cell,DBDValueController.EditType.NONE,null);
            PropertyCollector props=new PropertyCollector(valueController.getAttribute(),false);
            props.collectProperties();
            valueController.getValueHandler().contributeProperties(props,valueController);
            return props;
          }
        }
        return null;
      }
    }
);
    return page;
  }
 else   if (adapter == IFindReplaceTarget.class) {
    return getFindReplaceTarget();
  }
  return null;
}
