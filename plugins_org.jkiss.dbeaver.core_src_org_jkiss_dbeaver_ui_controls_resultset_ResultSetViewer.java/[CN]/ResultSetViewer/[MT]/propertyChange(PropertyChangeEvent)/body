{
  if (event.getProperty().equals(IThemeManager.CHANGE_CURRENT_THEME) || event.getProperty().equals(ThemeConstants.FONT_SQL_RESULT_SET) || event.getProperty().equals(ThemeConstants.COLOR_SQL_RESULT_SET_SELECTION_BACK)|| event.getProperty().equals(ThemeConstants.COLOR_SQL_RESULT_SET_SELECTION_FORE)|| event.getProperty().equals(ThemeConstants.COLOR_SQL_RESULT_SET_PREVIEW_BACK)) {
    applyThemeSettings();
  }
}
