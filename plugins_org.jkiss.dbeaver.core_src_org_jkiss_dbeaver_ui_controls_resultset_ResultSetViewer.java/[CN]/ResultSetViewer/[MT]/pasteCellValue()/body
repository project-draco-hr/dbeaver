{
  GridPos cell=getCurrentPosition();
  if (cell == null) {
    return;
  }
  cell=translateVisualPos(cell);
  DBDAttributeBinding metaColumn=model.getColumn(cell.col);
  if (isColumnReadOnly(metaColumn)) {
    return;
  }
  try {
    Object newValue=getColumnValueFromClipboard(metaColumn);
    if (newValue == null) {
      return;
    }
    new ResultSetValueController(cell,DBDValueController.EditType.NONE,null).updateValue(newValue);
  }
 catch (  Exception e) {
    UIUtils.showErrorDialog(site.getShell(),"Cannot replace cell value",null,e);
  }
}
