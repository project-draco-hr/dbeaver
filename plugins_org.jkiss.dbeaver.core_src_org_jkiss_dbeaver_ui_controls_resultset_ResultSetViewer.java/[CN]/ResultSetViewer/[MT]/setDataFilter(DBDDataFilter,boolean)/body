{
  if (!model.getDataFilter().equalFilters(dataFilter)) {
    if (model.setDataFilter(dataFilter)) {
      redrawData(true);
    }
    if (refreshData) {
      if (ResultSetUtils.isServerSideFiltering(this) && supportsDataFilter()) {
        refreshData(null);
      }
 else {
        activePresentation.formatData(true);
      }
    }
 else {
      updateFiltersText();
    }
  }
}
