{
  if (CommonUtils.equalObjects(model.getDataFilter(),dataFilter)) {
    return;
  }
  model.setDataFilter(dataFilter);
  if (refreshData) {
    reorderResultSet(true,new Runnable(){
      @Override public void run(){
        resetColumnOrdering();
      }
    }
);
  }
  this.updateFiltersText();
}
