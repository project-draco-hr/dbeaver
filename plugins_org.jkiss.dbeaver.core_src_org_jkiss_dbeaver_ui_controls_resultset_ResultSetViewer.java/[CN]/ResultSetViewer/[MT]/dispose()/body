{
  closeEditors();
  clearData();
  clearMetaData();
  if (!spreadsheet.isDisposed()) {
    spreadsheet.dispose();
  }
  statusLabel.dispose();
  themeManager.removePropertyChangeListener(ResultSetViewer.this);
  UIUtils.dispose(this.boldFont);
  if (toolBarManager != null) {
    toolBarManager.dispose();
  }
}
