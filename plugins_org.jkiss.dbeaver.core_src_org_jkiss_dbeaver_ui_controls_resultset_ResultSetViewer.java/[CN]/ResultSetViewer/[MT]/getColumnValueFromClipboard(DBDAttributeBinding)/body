{
  DBCSession session=getDataSource().openSession(VoidProgressMonitor.INSTANCE,DBCExecutionPurpose.UTIL,"Copy from clipboard");
  Object newValue;
  try {
    newValue=metaColumn.getValueHandler().getValueFromClipboard(session,metaColumn.getMetaAttribute(),getSpreadsheet().getClipboard());
  }
  finally {
    session.close();
  }
  return newValue;
}
