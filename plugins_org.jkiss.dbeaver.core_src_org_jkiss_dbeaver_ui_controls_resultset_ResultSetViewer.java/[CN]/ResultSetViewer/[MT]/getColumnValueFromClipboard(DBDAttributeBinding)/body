{
  DBCExecutionContext context=getDataSource().openContext(VoidProgressMonitor.INSTANCE,DBCExecutionPurpose.UTIL,"Copy from clipboard");
  Object newValue;
  try {
    newValue=metaColumn.getValueHandler().getValueFromClipboard(context,metaColumn.getAttribute(),getSpreadsheet().getClipboard());
  }
  finally {
    context.close();
  }
  return newValue;
}
