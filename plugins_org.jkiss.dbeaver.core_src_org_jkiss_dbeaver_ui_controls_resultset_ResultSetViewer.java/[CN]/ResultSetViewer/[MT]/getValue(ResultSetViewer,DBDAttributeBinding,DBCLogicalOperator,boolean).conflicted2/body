{
  if (useDefault) {
    return "..";
  }
 else {
    RowData focusRow=viewer.getFocusRow();
    if (focusRow == null) {
      return null;
    }
    FilterValueEditDialog dialog=new FilterValueEditDialog(viewer,column,focusRow,operator);
    if (dialog.open() == IDialogConstants.OK_ID) {
      return dialog.getValue();
    }
 else {
      return null;
    }
  }
}
