{
  if (element instanceof DBDAttributeBinding) {
    final DBSAttributeBase attribute=((DBDAttributeBinding)element).getAttribute();
switch (attribute.getDataKind()) {
case ARRAY:
      if (getGridMode() != GridMode.RECORD) {
        return null;
      }
    break;
case STRUCT:
case OBJECT:
  break;
case ANY:
break;
default :
return null;
}
}
return null;
}
