{
  if (!showCelIcons) {
    return null;
  }
  DBDAttributeBinding attr=(DBDAttributeBinding)(recordMode ? rowElement : colElement);
  if ((attr.getValueHandler().getFeatures() & DBDValueHandler.FEATURE_SHOW_ICON) != 0) {
    return DBUtils.getTypeImage(attr.getMetaAttribute());
  }
 else {
    return null;
  }
}
