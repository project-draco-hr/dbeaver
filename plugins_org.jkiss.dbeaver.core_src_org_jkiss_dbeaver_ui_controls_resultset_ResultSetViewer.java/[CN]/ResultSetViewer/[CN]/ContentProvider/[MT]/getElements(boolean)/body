{
  if (horizontal) {
    if (gridMode == GridMode.GRID) {
      return model.getVisibleColumns().toArray();
    }
 else {
      return new Object[]{model.getRow(curRowNum)};
    }
  }
 else {
    if (gridMode == GridMode.GRID) {
      return model.getAllRows().toArray();
    }
 else {
      return model.getVisibleColumns().toArray();
    }
  }
}
