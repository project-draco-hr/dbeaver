{
  ResultSetViewer rsv=(ResultSetViewer)ResultSetCommandHandler.getActiveResultSet(DBeaverUI.getActiveWorkbenchWindow().getActivePage().getActivePart());
  if (rsv == null) {
    return new IContributionItem[0];
  }
  List<IContributionItem> items=new ArrayList<>();
  for (  ResultSetPanelDescriptor panel : rsv.availablePanels) {
    items.add(new ActionContributionItem(new Action(panel.getId(),Action.AS_CHECK_BOX){
      @Override public void run(){
        super.run();
      }
    }
));
  }
  return items.toArray(new IContributionItem[items.size()]);
}
