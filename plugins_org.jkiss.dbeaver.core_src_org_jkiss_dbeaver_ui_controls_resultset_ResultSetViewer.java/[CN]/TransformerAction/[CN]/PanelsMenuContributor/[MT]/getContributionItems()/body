{
  IResultSetController rsv=ResultSetCommandHandler.getActiveResultSet(DBeaverUI.getActiveWorkbenchWindow().getActivePage().getActivePart());
  if (rsv == null) {
    return new IContributionItem[0];
  }
  return new IContributionItem[]{new ActionContributionItem(new Action("Some panel",Action.AS_CHECK_BOX){
    @Override public void run(){
      super.run();
    }
  }
)};
}
