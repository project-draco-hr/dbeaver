{
  if (valueType == Types.OTHER || valueType == Types.VARCHAR) {
    if ("BLOB".equalsIgnoreCase(typeName)) {
      return Types.BLOB;
    }
 else     if ("CLOB".equalsIgnoreCase(typeName)) {
      return Types.CLOB;
    }
 else     if ("NCLOB".equalsIgnoreCase(typeName)) {
      return Types.NCLOB;
    }
  }
  return valueType;
}
