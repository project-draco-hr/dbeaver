{
  try {
    Object newValue=extractEditorValue();
    ((IMultiController)valueController).closeInlineEditor();
    valueController.updateValue(newValue);
  }
 catch (  DBException e) {
    ((IMultiController)valueController).closeInlineEditor();
    UIUtils.showErrorDialog(null,"Value save","Can't save edited value",e);
  }
}
