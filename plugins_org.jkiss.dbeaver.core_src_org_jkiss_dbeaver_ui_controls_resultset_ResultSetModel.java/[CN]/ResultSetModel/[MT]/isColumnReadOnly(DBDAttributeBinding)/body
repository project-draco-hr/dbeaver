{
  if (column.getRowIdentifier() == null || !(column.getRowIdentifier().getEntity() instanceof DBSDataManipulator) || (column.getValueHandler().getFeatures() & DBDValueHandler.FEATURE_READ_ONLY) != 0) {
    return true;
  }
  DBSDataManipulator dataContainer=(DBSDataManipulator)column.getRowIdentifier().getEntity();
  return (dataContainer.getSupportedFeatures() & DBSDataManipulator.DATA_UPDATE) == 0;
}
