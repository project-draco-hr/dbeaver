{
  if (rows != null && !rows.isEmpty()) {
    int[] rowsToRemove=new int[rows.size()];
    int i=0;
    for (    RowInfo rowNum : rows)     rowsToRemove[i++]=rowNum.row;
    Arrays.sort(rowsToRemove);
    for (i=rowsToRemove.length; i > 0; i--) {
      cleanupRow(rowsToRemove[i - 1]);
    }
    rows.clear();
    return true;
  }
 else {
    return false;
  }
}
