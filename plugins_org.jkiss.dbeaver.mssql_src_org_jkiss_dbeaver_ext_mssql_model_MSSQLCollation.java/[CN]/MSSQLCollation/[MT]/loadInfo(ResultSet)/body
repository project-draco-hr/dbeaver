{
  this.name=JDBCUtils.safeGetString(dbResult,MSSQLConstants.COL_COLLATION);
  this.id=JDBCUtils.safeGetInt(dbResult,MSSQLConstants.COL_ID);
  this.isDefault="Yes".equalsIgnoreCase(JDBCUtils.safeGetString(dbResult,MSSQLConstants.COL_DEFAULT));
  this.isCompiled="Yes".equalsIgnoreCase(JDBCUtils.safeGetString(dbResult,MSSQLConstants.COL_COMPILED));
  this.sortLength=JDBCUtils.safeGetInt(dbResult,MSSQLConstants.COL_SORT_LENGTH);
}
