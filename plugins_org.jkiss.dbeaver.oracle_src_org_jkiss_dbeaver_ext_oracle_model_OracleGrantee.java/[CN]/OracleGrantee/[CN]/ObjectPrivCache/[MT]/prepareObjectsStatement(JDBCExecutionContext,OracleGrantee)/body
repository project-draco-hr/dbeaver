{
  final JDBCPreparedStatement dbStat=context.prepareStatement("SELECT p.*,o.OBJECT_TYPE\n" + "FROM DBA_TAB_PRIVS p, DBA_OBJECTS o\n" + "WHERE p.GRANTEE=? "+ "AND o.OWNER=p.OWNER AND o.OBJECT_NAME=p.TABLE_NAME AND o.OBJECT_TYPE<>'PACKAGE BODY'");
  dbStat.setString(1,owner.getName());
  return dbStat;
}
