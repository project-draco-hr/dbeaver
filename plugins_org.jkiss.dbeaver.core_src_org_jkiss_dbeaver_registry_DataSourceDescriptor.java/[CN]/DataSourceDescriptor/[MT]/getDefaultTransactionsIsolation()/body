{
  if (dataSource != null) {
    DBCTransactionManager txnManager=DBUtils.getTransactionManager(dataSource);
    if (txnManager != null) {
      try {
        return txnManager.getTransactionIsolation();
      }
 catch (      DBCException e) {
        log.debug("Can't determine isolation level",e);
        return null;
      }
    }
  }
  return null;
}
