{
  CreateEntityDialog dialog=new CreateEntityDialog(DBeaverUI.getActiveWorkbenchShell(),db2Schema.getDataSource(),DB2Messages.edit_db2_sequence_manager_dialog_title);
  if (dialog.open() != IDialogConstants.OK_ID) {
    return null;
  }
  return new DB2Sequence(db2Schema,dialog.getEntityName());
}
