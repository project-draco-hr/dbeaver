{
  final PreparedStatement dbStat=session.prepareStatement(sql);
  try {
    if (params != null) {
      for (int i=0; i < params.length; i++) {
        dbStat.setObject(i + 1,params[i]);
      }
    }
    ResultSet resultSet=dbStat.executeQuery();
    try {
      if (resultSet.next()) {
        return (T)resultSet.getObject(1);
      }
 else {
        return null;
      }
    }
  finally {
      resultSet.close();
    }
  }
  finally {
    dbStat.close();
  }
}
