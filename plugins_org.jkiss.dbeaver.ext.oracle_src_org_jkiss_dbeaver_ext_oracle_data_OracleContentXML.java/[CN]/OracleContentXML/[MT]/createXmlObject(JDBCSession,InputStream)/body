{
  try {
    return BeanUtils.invokeStaticMethod(OracleConstants.XMLTYPE_CLASS_NAME,"createXML",new Class[]{java.sql.Connection.class,java.io.InputStream.class},new Object[]{session.getOriginal(),stream});
  }
 catch (  SQLException e) {
    throw new DBCException(e,session.getDataSource());
  }
catch (  Throwable e) {
    throw new DBCException("Internal error when creating XMLType",e,session.getDataSource());
  }
}
