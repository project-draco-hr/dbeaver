{
  super(DBeaverActivator.getInstance().getPreferenceStore());
  try {
    File configurationFile=getConfigurationFile();
    if (configurationFile.exists()) {
      putValue(PREF_STORE_KEY,ContentUtils.readFileToString(configurationFile));
    }
  }
 catch (  IOException e) {
    log.error(e);
  }
}
