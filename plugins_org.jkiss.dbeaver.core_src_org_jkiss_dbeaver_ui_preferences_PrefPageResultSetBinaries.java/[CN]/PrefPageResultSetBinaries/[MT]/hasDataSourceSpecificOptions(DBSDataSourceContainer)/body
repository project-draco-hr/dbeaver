{
  DBPPreferenceStore store=dataSourceDescriptor.getPreferenceStore();
  return store.contains(DBeaverPreferences.MEMORY_CONTENT_MAX_SIZE) || store.contains(ModelPreferences.RESULT_SET_BINARY_PRESENTATION) || store.contains(DBeaverPreferences.RESULT_SET_BINARY_EDITOR_TYPE)|| store.contains(ModelPreferences.RESULT_SET_BINARY_STRING_MAX_LEN)|| store.contains(ModelPreferences.CONTENT_HEX_ENCODING)|| store.contains(ModelPreferences.CONTENT_CACHE_CLOB);
}
