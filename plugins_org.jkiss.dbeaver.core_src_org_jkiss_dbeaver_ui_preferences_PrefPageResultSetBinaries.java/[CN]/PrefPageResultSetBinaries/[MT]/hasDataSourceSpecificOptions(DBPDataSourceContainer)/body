{
  DBPPreferenceStore store=dataSourceDescriptor.getPreferenceStore();
  return store.contains(DBeaverPreferences.MEMORY_CONTENT_MAX_SIZE) || store.contains(ModelPreferences.RESULT_SET_BINARY_PRESENTATION) || store.contains(DBeaverPreferences.RESULT_SET_BINARY_EDITOR_TYPE)|| store.contains(ModelPreferences.RESULT_SET_BINARY_STRING_MAX_LEN)|| store.contains(ModelPreferences.CONTENT_HEX_ENCODING)|| store.contains(ModelPreferences.CONTENT_CACHE_CLOB)|| store.contains(DBeaverPreferences.RS_EDIT_LONG_AS_LOB)|| store.contains(DBeaverPreferences.RS_EDIT_MAX_TEXT_SIZE)|| store.contains(DBeaverPreferences.RS_COMMIT_ON_EDIT_APPLY)|| store.contains(DBeaverPreferences.RS_COMMIT_ON_CONTENT_APPLY);
}
