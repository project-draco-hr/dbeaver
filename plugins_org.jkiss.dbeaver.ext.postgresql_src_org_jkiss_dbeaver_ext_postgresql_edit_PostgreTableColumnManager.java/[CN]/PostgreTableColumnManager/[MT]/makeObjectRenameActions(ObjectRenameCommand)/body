{
  final PostgreAttribute column=command.getObject();
  return new DBEPersistAction[]{new SQLDatabasePersistAction("Rename column","ALTER TABLE " + DBUtils.getObjectFullName(column.getTable()) + " RENAME COLUMN "+ DBUtils.getQuotedIdentifier(column.getDataSource(),command.getOldName())+ " TO "+ DBUtils.getQuotedIdentifier(column))};
}
