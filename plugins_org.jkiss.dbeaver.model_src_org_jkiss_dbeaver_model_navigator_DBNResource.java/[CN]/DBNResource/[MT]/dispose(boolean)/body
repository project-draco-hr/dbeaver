{
  if (this.handler != null) {
    this.resource=null;
    this.handler=null;
    if (children != null) {
      for (      DBNNode child : children) {
        child.dispose(reflect);
      }
      children=null;
    }
    if (reflect) {
      getModel().fireNodeEvent(new DBNEvent(this,DBNEvent.Action.REMOVE,this));
    }
  }
  super.dispose(reflect);
}
