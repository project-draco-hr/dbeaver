{
  DBPDataSourceContainer dataSourceContainer=getDataSourceContainer();
  if (dataSourceContainer == null) {
    return super.getName();
  }
  return "Script: " + getFile().getName() + " \nConnection: "+ dataSourceContainer.getName()+ " \nType: "+ (dataSourceContainer.getDriver().getFullName())+ " \nURL: "+ dataSourceContainer.getConnectionConfiguration().getUrl();
}
