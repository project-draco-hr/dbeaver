{
  if (getObject() != null) {
    if (name.equals("targetType")) {
      try {
        Class<?> targetClass=Class.forName(value);
        return targetClass.isAssignableFrom(getObject().getClass());
      }
 catch (      ClassNotFoundException e) {
        log.warn("Unknown target type: " + value);
      }
    }
  }
  return false;
}
