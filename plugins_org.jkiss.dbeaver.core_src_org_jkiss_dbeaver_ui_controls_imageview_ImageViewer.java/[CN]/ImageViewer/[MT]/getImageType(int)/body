{
switch (type) {
case SWT.IMAGE_BMP:
    return "BMP";
case SWT.IMAGE_BMP_RLE:
  return "BMP RLE";
case SWT.IMAGE_GIF:
return "GIF";
case SWT.IMAGE_ICO:
return "ICO";
case SWT.IMAGE_JPEG:
return "JPEG";
case SWT.IMAGE_PNG:
return "PNG";
case SWT.IMAGE_TIFF:
return "TIFF";
case SWT.IMAGE_OS2_BMP:
return "OS2 BMP";
default :
return "UNKNOWN";
}
}
