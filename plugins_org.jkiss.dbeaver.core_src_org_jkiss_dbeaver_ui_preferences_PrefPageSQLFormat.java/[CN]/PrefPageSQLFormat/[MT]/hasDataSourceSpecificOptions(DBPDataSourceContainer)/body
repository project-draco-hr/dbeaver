{
  DBPPreferenceStore store=dataSourceDescriptor.getPreferenceStore();
  return store.contains(ModelPreferences.SQL_FORMAT_FORMATTER) || store.contains(ModelPreferences.SQL_FORMAT_KEYWORD_CASE) || store.contains(ModelPreferences.SQL_FORMAT_KEYWORD_CASE_AUTO)|| store.contains(ModelPreferences.SQL_FORMAT_EXTERNAL_CMD);
}
