{
  DBPPreferenceStore store=dataSourceDescriptor.getPreferenceStore();
  return store.contains(ModelPreferences.SQL_FORMAT_FORMATTER) || store.contains(ModelPreferences.SQL_FORMAT_KEYWORD_CASE) || store.contains(ModelPreferences.SQL_FORMAT_EXTERNAL_CMD)|| store.contains(ModelPreferences.SQL_FORMAT_EXTERNAL_FILE)|| store.contains(ModelPreferences.SQL_FORMAT_EXTERNAL_TIMEOUT);
}
