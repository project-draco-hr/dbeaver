{
  DBPPreferenceStore store=dataSourceDescriptor.getPreferenceStore();
  return store.contains(SQLPreferenceConstants.FORMAT_KEYWORD_CASE) || store.contains(SQLPreferenceConstants.FORMAT_KEYWORD_CASE_AUTO) || store.contains(SQLPreferenceConstants.FORMAT_EXTERNAL)|| store.contains(SQLPreferenceConstants.FORMAT_EXTERNAL_CMD);
}
