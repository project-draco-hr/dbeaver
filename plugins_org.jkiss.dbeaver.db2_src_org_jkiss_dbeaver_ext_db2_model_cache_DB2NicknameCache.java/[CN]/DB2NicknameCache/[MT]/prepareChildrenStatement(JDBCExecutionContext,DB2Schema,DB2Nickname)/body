{
  String sql;
  if (forNickname != null) {
    sql=SQL_COLS_NICK;
  }
 else {
    sql=SQL_COLS_ALL;
  }
  JDBCPreparedStatement dbStat=context.prepareStatement(sql);
  dbStat.setString(1,db2Schema.getName());
  if (forNickname != null) {
    dbStat.setString(2,forNickname.getName());
  }
  return dbStat;
}
