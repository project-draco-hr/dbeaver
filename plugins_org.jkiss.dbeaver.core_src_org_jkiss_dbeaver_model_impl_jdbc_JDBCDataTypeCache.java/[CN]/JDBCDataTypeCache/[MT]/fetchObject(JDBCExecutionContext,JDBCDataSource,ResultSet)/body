{
  return new JDBCDataType(this.owner,JDBCUtils.safeGetInt(dbResult,JDBCConstants.DATA_TYPE),JDBCUtils.safeGetString(dbResult,JDBCConstants.TYPE_NAME),JDBCUtils.safeGetString(dbResult,JDBCConstants.LOCAL_TYPE_NAME),JDBCUtils.safeGetBoolean(dbResult,JDBCConstants.UNSIGNED_ATTRIBUTE),JDBCUtils.safeGetInt(dbResult,JDBCConstants.SEARCHABLE) != 0,JDBCUtils.safeGetInt(dbResult,JDBCConstants.PRECISION),JDBCUtils.safeGetInt(dbResult,JDBCConstants.MINIMUM_SCALE),JDBCUtils.safeGetInt(dbResult,JDBCConstants.MAXIMUM_SCALE));
}
