{
  JDBCPreparedStatement dbStat=session.prepareStatement("SELECT " + OracleUtils.getSysCatalogHint(owner.getDataSource()) + " * FROM SYS.ALL_OBJECTS "+ "WHERE OBJECT_TYPE IN ('PROCEDURE','FUNCTION') "+ "AND OWNER=? "+ "ORDER BY OBJECT_NAME");
  dbStat.setString(1,owner.getName());
  return dbStat;
}
