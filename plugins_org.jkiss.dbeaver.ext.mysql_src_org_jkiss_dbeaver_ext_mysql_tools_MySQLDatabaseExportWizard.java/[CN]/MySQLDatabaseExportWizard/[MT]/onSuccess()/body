{
  UIUtils.showMessageBox(getShell(),MySQLMessages.tools_db_export_wizard_title,NLS.bind(MySQLMessages.tools_db_export_wizard_message_export_completed,getDatabaseObject().getName()),SWT.ICON_INFORMATION);
  RuntimeUtils.launchProgram(outputFile.getAbsoluteFile().getParentFile().getAbsolutePath());
}
