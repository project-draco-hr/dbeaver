{
  super(dataSource,dbResult != null);
  if (dbResult != null) {
    this.id=JDBCUtils.safeGetLong(dbResult,"USER_ID");
    this.name=JDBCUtils.safeGetString(dbResult,"USERNAME");
    if (this.name == null) {
      log.warn("Empty schema name fetched");
      this.name="? " + super.hashCode();
    }
  }
}
