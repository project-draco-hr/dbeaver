{
  final CommandContributionItemParameter contributionParameters=new CommandContributionItemParameter(serviceLocator,null,commandId,null,image == null ? null : DBeaverIcons.getImageDescriptor(image),null,null,name,null,toolTip,CommandContributionItem.STYLE_PUSH,null,false);
  if (showText) {
    contributionParameters.mode=CommandContributionItem.MODE_FORCE_TEXT;
  }
  return new CommandContributionItem(contributionParameters);
}
