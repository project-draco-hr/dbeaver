{
  if (typeId <= 0) {
    return null;
  }
  for (  PostgreSchema schema : getDatabase().schemaCache.getCachedObjects()) {
    final PostgreDataType dataType=schema.dataTypeCache.getDataType(typeId);
    if (dataType != null) {
      return dataType;
    }
  }
  log.debug("Data type '" + typeId + "' not found");
  return null;
}
