{
  DBPImage image=null;
  if (object instanceof DBPImageProvider) {
    image=((DBPImageProvider)object).getObjectImage();
  }
  if (image == null) {
    if (object instanceof DBSTypedObject) {
      image=getTypeImage((DBSTypedObject)object);
    }
    if (image == null) {
      image=DBIcon.TYPE_OBJECT;
    }
  }
  return image;
}
