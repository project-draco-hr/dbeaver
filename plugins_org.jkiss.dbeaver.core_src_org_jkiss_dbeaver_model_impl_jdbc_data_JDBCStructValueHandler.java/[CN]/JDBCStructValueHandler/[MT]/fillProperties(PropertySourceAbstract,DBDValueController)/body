{
  try {
    Object value=controller.getValue();
    if (value instanceof JDBCStruct) {
      propertySource.addProperty("sql_type",CoreMessages.model_jdbc_type_name,((JDBCStruct)value).getTypeName());
    }
  }
 catch (  Exception e) {
    log.warn("Could not extract struct value information",e);
  }
}
