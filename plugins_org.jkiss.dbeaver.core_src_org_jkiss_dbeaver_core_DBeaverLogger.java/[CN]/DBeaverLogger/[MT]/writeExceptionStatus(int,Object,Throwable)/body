{
  if (t == null) {
    DBeaverActivator.getInstance().getLog().log(new Status(severity,corePluginID,message == null ? null : message.toString()));
  }
 else {
    DBeaverActivator.getInstance().getLog().log(new MultiStatus(corePluginID,0,new IStatus[]{RuntimeUtils.makeExceptionStatus(severity,t)},message == null ? null : message.toString(),t));
  }
}
