{
  object=DBUtils.getPublicObject(object);
  if (object == null) {
    return false;
  }
  Object adapted=adaptType(object);
  for (  ObjectType objectType : objectTypes) {
    if (objectType.appliesTo(object) || (adapted != null && objectType.appliesTo(adapted))) {
      return true;
    }
  }
  return false;
}
