{
  return CommonUtils.equalObjects(attr1.getLabel(),attr2.getLabel()) && CommonUtils.equalObjects(attr1.getName(),attr2.getName()) && CommonUtils.equalObjects(attr1.getEntityMetaData(),attr2.getEntityMetaData())&& attr1.getOrdinalPosition() == attr2.getOrdinalPosition() && attr1.isRequired() == attr2.isRequired() && attr1.getMaxLength() == attr2.getMaxLength() && attr1.getPrecision() == attr2.getPrecision() && attr1.getScale() == attr2.getScale() && attr1.getTypeID() == attr2.getTypeID() && CommonUtils.equalObjects(attr1.getTypeName(),attr2.getTypeName());
}
