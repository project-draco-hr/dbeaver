{
  String tableName=valueController.getAttributeMetaData().getEntityName();
  String inputName=CommonUtils.isEmpty(tableName) ? valueController.getAttributeMetaData().getName() : tableName + "." + valueController.getAttributeMetaData().getName();
  if (isReadOnly()) {
    inputName+=" [Read Only]";
  }
  return inputName;
}
