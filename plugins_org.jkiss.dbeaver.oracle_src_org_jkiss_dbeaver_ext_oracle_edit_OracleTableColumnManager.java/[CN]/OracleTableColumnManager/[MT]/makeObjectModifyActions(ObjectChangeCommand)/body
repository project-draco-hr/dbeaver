{
  final OracleTableColumn column=command.getObject();
  return new IDatabasePersistAction[]{new AbstractDatabasePersistAction(OracleMessages.edit_oracle_table_column_manager_action_alter_table_column,"ALTER TABLE " + column.getTable().getFullQualifiedName() + " MODIFY "+ getNestedDeclaration(column.getTable(),command))};
}
