{
  IPropertySource source=getSource();
  if (source == null) {
    return null;
  }
  final Object object=source.getEditableValue();
  if (!isEditable(object)) {
    return null;
  }
  if (valueEditor != null) {
    return valueEditor.createCellEditor(parent,object,propInfo);
  }
 else {
    return createCellEditor(parent,object,this);
  }
}
