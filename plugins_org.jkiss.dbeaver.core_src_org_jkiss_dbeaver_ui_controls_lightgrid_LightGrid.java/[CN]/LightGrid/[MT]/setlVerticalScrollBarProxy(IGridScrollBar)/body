{
  checkWidget();
  if (getVerticalBar() != null) {
    return;
  }
  vScroll=scroll;
  vScroll.addSelectionListener(new SelectionListener(){
    @Override public void widgetSelected(    SelectionEvent e){
      onScrollSelection();
    }
    @Override public void widgetDefaultSelected(    SelectionEvent e){
    }
  }
);
}
