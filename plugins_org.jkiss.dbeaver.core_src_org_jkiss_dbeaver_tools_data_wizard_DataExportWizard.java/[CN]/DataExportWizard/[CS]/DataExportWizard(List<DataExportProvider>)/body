{
  this.settings=new DataExportSettings(dataContainers);
  IDialogSettings workbenchSettings=DBeaverActivator.getInstance().getDialogSettings();
  IDialogSettings section=workbenchSettings.getSection(RS_EXPORT_WIZARD_DIALOG_SETTINGS);
  if (section == null) {
    section=workbenchSettings.addNewSection(RS_EXPORT_WIZARD_DIALOG_SETTINGS);
  }
  setDialogSettings(section);
  settings.loadFrom(section);
}
