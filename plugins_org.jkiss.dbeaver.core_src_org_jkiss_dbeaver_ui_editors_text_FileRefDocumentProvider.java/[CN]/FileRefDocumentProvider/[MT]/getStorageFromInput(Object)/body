{
  if (element instanceof IEditorInput) {
    return ContentUtils.getFileFromEditorInput((IEditorInput)element);
  }
  if (element instanceof IAdaptable) {
    IStorage storage=(IStorage)((IAdaptable)element).getAdapter(IStorage.class);
    if (storage != null) {
      return storage;
    }
  }
  return null;
}
