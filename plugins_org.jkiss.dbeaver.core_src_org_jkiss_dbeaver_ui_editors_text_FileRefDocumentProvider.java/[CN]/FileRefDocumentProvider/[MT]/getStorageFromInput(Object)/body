{
  if (element instanceof IAdaptable) {
    IStorage storage=((IAdaptable)element).getAdapter(IStorage.class);
    if (storage != null) {
      return storage;
    }
  }
  if (element instanceof IEditorInput) {
    IFile file=EditorUtils.getFileFromEditorInput((IEditorInput)element);
    if (file != null) {
      return file;
    }
  }
  return null;
}
