{
  if (element instanceof IAdaptable) {
    IFile file=(IFile)((IAdaptable)element).getAdapter(IFile.class);
    if (file != null) {
      return file;
    }
    IStorage storage=(IStorage)((IAdaptable)element).getAdapter(IStorage.class);
    if (storage != null) {
      return storage;
    }
  }
  return null;
}
