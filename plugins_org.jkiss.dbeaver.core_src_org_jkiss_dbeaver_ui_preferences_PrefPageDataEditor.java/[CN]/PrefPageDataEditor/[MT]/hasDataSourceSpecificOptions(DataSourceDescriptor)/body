{
  AbstractPreferenceStore store=dataSourceDescriptor.getPreferenceStore();
  return store.contains(DBeaverPreferences.RS_EDIT_USE_ALL_COLUMNS) || store.contains(DBeaverPreferences.RS_EDIT_MAX_TEXT_SIZE) || store.contains(DBeaverPreferences.RS_EDIT_LONG_AS_LOB)|| store.contains(DBeaverPreferences.RS_COMMIT_ON_EDIT_APPLY)|| store.contains(DBeaverPreferences.RS_COMMIT_ON_CONTENT_APPLY);
}
